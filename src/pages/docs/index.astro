import LeftSidebar from "../../components/LeftSidebar.astro";
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <body>
    <Header homel="./" docsl="./docs.html" membersl="./members.html" />
